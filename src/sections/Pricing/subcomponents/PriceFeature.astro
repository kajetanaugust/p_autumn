---
import Text from "../../../components/Text/Text.astro";
import {Check} from "lucide-astro";
import Box from "../../../components/Box/Box.astro";

interface Props {
    text: string;
}

const {text} = Astro.props;
---

<Box direction="row" gap={8} width="fit-content">
    <Check/>
    <Text variant="text-regular">
        {text}
    </Text>
</Box>
